{
    "name": "andri-sudarmawijaya/theme-bootstrap",
    "type": "codeigniter-view",
    "description": "codeigniter-packages/theme-bootstrap",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AndriSudarmawijaya\\ThemeBootstrap\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jooonwood",
            "email": "jooonwood@gmail.com"
        }
    ],
    "require": {
        "andri-sudarmawijaya/theme-bootstrap": "dev-master",
        "coreui/coreui": "^4.0",
        "twbs/bootstrap": "^4.0",
        "datatables/datatables": "^1.10",
        "fortawesome/font-awesome": " ^5.15",
        "snapappointments/bootstrap-select": " ^1.13"
    },
    "scripts": {
        "post-install-cmd": [
            "@post-install"
        ],
        "post-install": [
